FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    in1
    {
        type		turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.014;
        value           $internalField;
    }

    in2
    {
        type		turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.014;
        value           $internalField;
    }

    out
    {
        type            zeroGradient;
    }
    
    walls
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    frontandback
    {
        type           empty;
    }
}


