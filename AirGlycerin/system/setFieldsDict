FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.glycerin 0
);

regions
(
    boxToCell
    {
        box (0.01 0 -0.1) (0.1 0.02 0.1);
        fieldValues
        (
            volScalarFieldValue alpha.glycerin 1
        );
    }
);


