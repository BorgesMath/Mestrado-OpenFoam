actions
(
    {
        name    porousCellSet; 
        type    cellSet;
        action  new;
        
        source  boxToCell;
        sourceInfo
        {
            box (0.01 0 -0.1) (0.1 0.02 0.1);
        }
    }
    {
        name    porousCells;
        type    cellZoneSet;
        action  new;
        
        source  setToCellZone;
        sourceInfo
        {
            set porousCellSet;
        }
    }
);

