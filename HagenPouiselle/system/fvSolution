FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        preconditioner  DIC;
        tolerance       1e-08;
        relTol          0.00001;
        smoother        GaussSeidel;
        nPreSweeps      0;
        nPostSweeps     2;
        cacheAgglomeration  on;
        agglomerator    faceAreaPair;
        nCellsinCoarsestLevel   100;
        mergeLevels     1;
    }


    U
    {
        solver          PBiCG;
        smoother        DILU;
        tolerance       1e-08;
        relTol          0;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 1;
            momentumPredictor no;
    
    residualControl
    {
         p  1e-6;
         U  1e-3;
    }
}

relaxationFactors
{
    fields
    {
        p 0.3;
    }
    equations
    {
        U 0.7;
    }

}


